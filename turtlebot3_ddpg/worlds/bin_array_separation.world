<sdf version='1.4'>
  <world name='default'>
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <physics type="ode">
      <real_time_update_rate>1000.0</real_time_update_rate>
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1</real_time_factor>
      <ode>
        <solver>
          <type>quick</type>
          <iters>150</iters>
          <precon_iters>0</precon_iters>
          <sor>1.400000</sor>
          <use_dynamic_moi_rescaling>1</use_dynamic_moi_rescaling>
        </solver>
        <constraints>
          <cfm>0.00001</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>2000.000000</contact_max_correcting_vel>
          <contact_surface_layer>0.01000</contact_surface_layer>
        </constraints>
      </ode>
    </physics>

    <!-- goal -->
    <model name='goal'>
      <include>
          <uri>model://goal</uri>
      </include>
      <pose>3 0 0 0 0 0</pose>
    </model>
  
    <!-- static populations -->
    <population name="population_1">
      <model name="population_1">
        <include>
          <static>true</static>
          <uri>model://cylinder_bin</uri>
        </include>
      </model>
      <pose>0.5 2 0 0 0 0</pose>
      <box>
        <size>3 1 0.01</size>
      </box>
      <model_count>3</model_count>
      <distribution>
        <type>linear-x</type>
      </distribution>
    </population>

    <population name="population_2">
      <model name="population_2">
        <include>
          <static>true</static>
          <uri>model://cylinder_bin</uri>
        </include>
      </model>
      <pose>0.5 1 0 0 0 0</pose>
      <box>
        <size>5 1 0.01</size>
      </box>
      <model_count>5</model_count>
      <distribution>
        <type>linear-x</type>
      </distribution>
    </population> 

    <population name="population_3">
      <model name="population_3">
        <include>
          <static>true</static>
          <uri>model://cylinder_bin</uri>
        </include>
      </model>
      <pose>-1 0 0 0 0 0</pose>
      <box>
        <size>2 1 0.01</size>
      </box>
      <model_count>2</model_count>
      <distribution>
        <type>linear-x</type>
      </distribution>
    </population>

    <population name="population_4">
      <model name="population_4">
        <include>
          <static>true</static>
          <uri>model://cylinder_bin</uri>
        </include>
      </model>
      <pose>2 0 0 0 0 0</pose>
      <box>
        <size>2 1 0.01</size>
      </box>
      <model_count>2</model_count>
      <distribution>
        <type>linear-x</type>
      </distribution>
    </population>
  
    <population name="population_5">
      <model name="population_5">
        <include>
          <static>true</static>
          <uri>model://cylinder_bin</uri>
        </include>
      </model>
      <pose>0.5 -1 0 0 0 0</pose>
      <box>
        <size>5 1 0.01</size>
      </box>
      <model_count>5</model_count>
      <distribution>
        <type>linear-x</type>
      </distribution>
