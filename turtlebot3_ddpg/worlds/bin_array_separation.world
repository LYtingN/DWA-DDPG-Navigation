    <!-- dynamic cylinders -->
    <model name='dynamic_cylinder_0'>
      <include>
          <uri>model://cylinder_bin_grey</uri>
      </include>
      <pose>10 10 0 0 0 0</pose>
    </model>

    <model name='dynamic_cylinder_1'>
